  "bin": {
    "env-prompt": "./dist/index.js"
  },
  "author": "Brian Kotos <brian.kotos@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^10.17.13",
    "ts-loader": "^5.4.5",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10"
  }
}
