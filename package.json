{
  "name": "env-prompt",
  "version": "1.1.1",
  "description": "A dependency-free utility that prompts you for your project's environment variables.",
  "main": "index.js",
  "scripts": {
    "start": "webpack --mode=development --watch",
    "build-dist": "webpack --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/env-prompt/env-prompt.git"
  },
  "keywords": [
    ".env",
    "env",
    "parameters",
    "environment",
    "variables",
    "configuration",
    "config",
    "interactive",
